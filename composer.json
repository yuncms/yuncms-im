{
  "name": "yuncms/yuncms-im",
  "description": "The im module for the yuncms.",
  "keywords": [
    "yii",
    "yii2",
    "yii2-extension",
    "yuncms"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.13",
    "yiisoft/yii2-queue": "~2.0.1",
    "xutl/yii2-tim": "~2.0.0",
    "yuncms/yuncms-user": "~2.0.0"
  },
  "require-dev": {
    "codeception/base": "^2.2.3",
    "codeception/verify": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\im\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "id": "im",
      "translate": {
        "class": "yii\\i18n\\PhpMessageSource",
        "sourceLanguage": "en-US",
        "basePath": "@yuncms/im/messages"
      },
      "migrationNamespace": "yuncms\\im\\migrations",
      "backend": {
        "class": "yuncms\\im\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\im\\frontend\\Module"
      }
    },
    "bootstrap": "yuncms\\im\\Bootstrap",
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
